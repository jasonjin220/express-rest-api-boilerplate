{
  "name": "node-express-es6-starter",
  "version": "1.0.0",
  "private": true,
  "description": "Node/Express.js start project supporting es6 module",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon src/app.js --exec babel-node",
    "build": "babel src -d dist --copy-files",
    "postinstall": "npm run build"
  },
  "keywords": [
    "Node",
    "Express",
    "RestAPI",
    "MongoDB",
    "Mongoose",
    "ES6",
    "Starter"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "debug": "^2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "nodemon": "^2.0.9"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
